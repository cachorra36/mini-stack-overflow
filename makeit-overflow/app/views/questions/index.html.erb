
<h1>Mini Stack Overflow</h1>

<h2>Questions</h2>

	<%= render "form" %>

<ul>
	<% @questions.each do |question| %>


	<li><%= link_to question.title, question_path(question) %> <%= link_to 'Edit', edit_question_path(question) %> <%= link_to 'Delete', question_path(question), method: :delete %></li> 
	
	<% end %>
</ul>

