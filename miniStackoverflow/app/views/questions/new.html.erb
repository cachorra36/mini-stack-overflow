<%= form_for @question do |f| %>

<p>
  <%= f.label :title  %><br>
  <%= f.text_field :title  %>
</p>
<p>
  <%= f.label :content %><br>
  <%= f.text_area :content  %>
</p>
<p>
  <%= f.label :user  %><br>
  <%= f.text_field :user  %>
</p>
<p>
  <%= f.sumbit "Create Question" %>
  <%= f.sumbit %>
</p>

<% end %>

